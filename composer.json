{
  "name": "woolf/carter",
  "description": "Shopify app jumpstart for Laravel",
  "license": "MIT",
  "homepage": "https://github.com/nickywoolf/carter",
  "autoload": {
    "psr-4": {
      "Woolf\\Carter\\": "src/"
    }
  },
  "require": {
    "php": ">=5.5.9",
    "guzzlehttp/guzzle": "^6.1",
    "symfony/http-foundation": "^3.0",
    "illuminate/contracts": "^5.1",
    "illuminate/support": "^5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "^0.9.4"
  },
  "autoload-dev": {
    "psr-4": {
      "Woolf\\Carter\\Tests\\": "tests/"
    }
  }
}
